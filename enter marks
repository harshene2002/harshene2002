#include <stdio.h>
main()
{
int num;
printf("Enter your mark ");
scanf("%d",&num);
printf(" You entered %d", num); 
	else if(num 80>100){
	printf("You got A grade"); 
		}
	else if ( num 70>84){ 
		printf("You got B grade");
		}
	else if ( num 55>69){
		printf("You got C grade");
		}
	else if ( num 40<54){
		printf("You got d grade");
		}
		else if ( num < 40){
		printf("You got f grade");
		}
return 0;
}
